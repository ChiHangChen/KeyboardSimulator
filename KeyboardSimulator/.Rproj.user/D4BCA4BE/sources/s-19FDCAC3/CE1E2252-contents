\name{mouse.click}
\alias{mouse.click}
\title{Simulate mouse click}
\usage{
mouse.click(button = "left", hold = F)
}
\arguments{
\item{button}{character. The allowed values are "\code{left}" and "\code{right}"}
}
\description{
Simulate mouse click
}
\examples{
## Move to the third working window
}
